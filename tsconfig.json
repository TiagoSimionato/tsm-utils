{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "dist",
    "noEmit": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "allowJs": false,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  }
}
